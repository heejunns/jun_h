n = int(input())
x = 1
y = []
z = []
h = 0
while x <=(2*n):
    y.append(" ")
    z.append(" ")
    x+=1
x = 1

while h < (2*n):
    if (h%2) == 0 :
        y[h] = "*"
        h+=1
    elif (h%2) == 1 : 
        z[h] = "*"
        h+=1

while x <=n:
    if (x%2) == 1:
        print("".join(y[:(len(y)-1)]))
        x+=1
    elif (x%2) == 0 :
        print("".join(z))
        x+=1
        

