n = int(input()) # 거슴름돈 액수 
x = n//5 # 최대 5원으로 줄수 있는 개수 
y = []
for i in range(x+1): # 5원을 0개부터 x개까지 거슬러주기
   total = 0 
   if (n-(5*i)) % 2 == 0 : # 2원으로 나눠줬을때 0 원인지 확인 
      total +=(i+((n-(5*i))//2))
      y.append(total) # 거슬러줄수있는 가능한 경우의 수 모두 y 리스트에 저장 
if y == []: # 거슬러줬을때 모든 경우의 수가 0원이 안되기 때문에 y 리스트가 비어있는거
   print(-1) # 그래서 -1 출력
else:
   print(min(y)) # 그중에 가장 최소 개수 출력

