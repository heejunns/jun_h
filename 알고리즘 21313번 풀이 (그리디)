n = int(input())
x = 1
q = 1
y = []
if n%2 == 0 :
    while x <= n:
        if x%2 ==1:
            y.append(q)
            x+=1
        elif x%2 == 0:
            y.append(q+1)
            x+=1
elif n%2 == 1:
    while x <= n:
        if x == n:
            y.append(q+2)
            x+=1
        elif x%2 == 1:
            y.append(q)
            x+=1
        elif x%2 == 0:
            y.append(q+1)
            x+=1
for i in y:
    print(i,end=" ")
        
        


